# Database
DATABASE_URL="postgresql://user:password@localhost:5432/sellio"

# JWT Secret (generate a random string)
JWT_SECRET="your-secret-key-here"

# Server Port
PORT=3001

# YooKassa (ЮКасса) Integration
# Получите эти данные в личном кабинете ЮКассы: https://yookassa.ru/
YOOKASSA_SHOP_ID="your_shop_id"
YOOKASSA_SECRET_KEY="your_secret_key"

# URL для вебхуков ЮКассы (настройте в личном кабинете ЮКассы)
# Пример: https://yourdomain.com/api/webhooks/yookassa
YOOKASSA_WEBHOOK_URL="https://yourdomain.com/api/webhooks/yookassa"
